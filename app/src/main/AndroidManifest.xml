<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.vikzpizza"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk android:targetSdkVersion="18" />
    <uses-permission android:name="android.permission.CALL_PHONE"/>
    <uses-permission android:name="android.permission.SEND_SMS"/>
           

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.vikzpizza.Home"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.vikzpizza.Registration"
            android:label="@string/title_activity_registration" >
        </activity>
        <activity
            android:name="com.example.vikzpizza.Fill_order"
            android:label="@string/title_activity_fill_order" >
        </activity>
        <activity
            android:name="com.example.vikzpizza.Options"
            android:label="@string/title_activity_options" >
        </activity>
        <activity
            android:name="com.example.vikzpizza.Confirm"
            android:label="@string/title_activity_confirm" >
        </activity>
    </application>

</manifest>
